#include<stdio.h>
int find_lcm(int,int);
int main()
{
    int a,b,lcm;
    printf("\n \n enter two integers to find lcm of:\n");
    scanf("%d %d", &a,&b);
    lcm = find_lcm(a,b);
    printf("\n\n lcm of %d and %d is : %d \n\n",a,b,lcm);
    return 0;
}
int find_lcm(int a,int b)
{
    static int temp = 1;
    if(temp %a==0 && temp %b==0)
     {
         return 0;
     }
     else
     {
         temp++;
         find_lcm(a,b);
         return temp;
     }
    
}
